<manifest>
   <ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Topic Title 1">
      <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
         <Content>How to subtract numbers</Content>
      </CharacterStyleRange>
   </ParagraphStyleRange>
   <ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Topic Title 1">
      <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
         <Br/>
      </CharacterStyleRange>
   </ParagraphStyleRange>
   <ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/Short Description">
      <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/$ID/[No character style]">
